{% extends '@SbSAdminLTE/Layout/base.html.twig' %}

  {% block corestyles %}
  <meta property="og:title" content="I7sa" />
  <meta property="og:url" content="https://i7sa.com/" />
  <meta property="og:image" content="https://i7sa.com/images/logo.png" />
  <meta property="og:description" content="" />
  <meta property="og:site_name" content="I7sa" />
      {{ parent() }}
  {% endblock %}

  {% block title %}Style de Vie{% endblock %}

  {% block stylesheets %}
      <!-- Tempusdominus Bootstrap 4 -->
      <link rel="stylesheet" href="{{ asset('bundles/sbsadminlte/plugins/tempusdominus-bootstrap-4/css/tempusdominus-bootstrap-4.min.css') }}" >
      <!-- iCheck -->
      <link rel="stylesheet"  href="{{ asset('bundles/sbsadminlte/plugins/icheck-bootstrap/icheck-bootstrap.min.css') }}">
      <!-- JQVMap -->
      <link rel="stylesheet" href="{{ asset('bundles/sbsadminlte/plugins/jqvmap/jqvmap.min.css') }}" >
      <!-- Daterange picker -->
      <link rel="stylesheet" href="{{ asset('bundles/sbsadminlte/plugins/daterangepicker/daterangepicker.css') }}">
      <!-- summernote -->
      <link rel="stylesheet" href="{{ asset('bundles/sbsadminlte/plugins/summernote/summernote-bs4.min.css') }}" >

      <!-- Select2 -->
      <link rel="stylesheet" href="{{ asset('bundles/sbsadminlte/plugins/select2/css/select2.min.css') }}" >
      <link rel="stylesheet" href="{{ asset('bundles/sbsadminlte/plugins/select2-bootstrap4-theme/select2-bootstrap4.min.css') }}" >

      <!-- DataTables -->
      <link rel="stylesheet" href="{{ asset('bundles/sbsadminlte/plugins/datatables-bs4/css/dataTables.bootstrap4.min.css') }}" >
      <link rel="stylesheet" href="{{ asset('bundles/sbsadminlte/plugins/datatables-responsive/css/responsive.bootstrap4.min.css') }}" >
      <link rel="stylesheet" href="{{ asset('bundles/sbsadminlte/plugins/datatables-buttons/css/buttons.bootstrap4.min.css') }}" >
      {% block stylesheets_surcharge %}{% endblock %}
      <link rel="stylesheet" href="{{ asset('css/override_base_page.css') }}" >

  {% endblock %}

  {% block corejs %}
      {{ parent() }}
  {% endblock %}

  {% block fonts %}
      {{ parent() }}
  {% endblock %}

  {% block admin_lte_navbar %}
      <nav class="main-header navbar navbar-expand navbar-white navbar-light">
          <ul class="navbar-nav">
              <li class="nav-item">
                  {{ sidebar_toggle_button() }}
              </li>
          </ul>
          {% if app.user is not null and is_granted('IS_AUTHENTICATED_REMEMBERED') %}
              {# Top navbar links #}
              {{ navbar_menu(app.request) }}
              {# Right navbar links #}
              <ul class="navbar-nav ml-auto">
                  {# Notifications Dropdown Menu #}
                  {{ navbar_notifications() }}
                  {# Tasks Dropdown Menu #}
                  {{ navbar_tasks() }}
                  {# User Dropdown Menu #}
                  {{ navbar_user_account() }}
              </ul>
          {% endif %}
      </nav>
  {% endblock %}

  {% block admin_lte_logo %}
      <a href="{{ path('homepage') }}" class="brand-link">
          <img src="{{ asset('img/logo.png') }}"
               alt="AdminLTE Logo"
               class="brand-image img-circle elevation-3"
               style="opacity: .8;    background: #fff;">
          <span class="brand-text font-weight-light">Style de Vie</span>
      </a>
  {% endblock %}
  {# Sidebar #}
  {% block admin_lte_sidebar %}
      {% if app.user is not null and is_granted('IS_AUTHENTICATED_REMEMBERED') %}
          <div class="sidebar">
              <nav class="mt-2">
                  {{ sidebar_menu(app.request) }}
              </nav>
          </div>
      {% endif %}
  {% endblock %}

   {% block admin_lte_footer %}
       <footer class="main-footer">
           <div class="float-right d-none d-sm-block"><b>Style de Vie</b></div>
           <strong>Copyright &copy; {{ "now"|date("Y") }}.</strong>
           tous droits réservés.
       </footer>
   {% endblock %}

   {% block javascripts %}
    <!-- ChartJS -->
    <script src="{{ asset('bundles/sbsadminlte/plugins/chart.js/Chart.min.js') }}"></script>
    <!-- Sparkline -->
    <script src="{{ asset('bundles/sbsadminlte/plugins/sparklines/sparkline.js') }}"></script>
    <!-- JQVMap -->
    <script src="{{ asset('bundles/sbsadminlte/plugins/jqvmap/jquery.vmap.min.js') }}"></script>
    <script src="{{ asset('bundles/sbsadminlte/plugins/jqvmap/maps/jquery.vmap.usa.js') }}"></script>
    <!-- jQuery Knob Chart -->
    <script src="{{ asset('bundles/sbsadminlte/plugins/jquery-knob/jquery.knob.min.js') }}"></script>
    <!-- daterangepicker -->
    <script src="{{ asset('bundles/sbsadminlte/plugins/moment/moment.min.js') }}"></script>
    <script src="{{ asset('bundles/sbsadminlte/plugins/daterangepicker/daterangepicker.js') }}"></script>
    <!-- Tempusdominus Bootstrap 4 -->
    <script src="{{ asset('bundles/sbsadminlte/plugins/tempusdominus-bootstrap-4/js/tempusdominus-bootstrap-4.min.js') }}"></script>
    <!-- Summernote -->
    <script src="{{ asset('bundles/sbsadminlte/plugins/summernote/summernote-bs4.min.js') }}"></script>

    <script src="{{ asset('bundles/sbsadminlte/plugins/datatables/jquery.dataTables.min.js') }}" ></script>
    <script src="{{ asset('bundles/sbsadminlte/plugins/datatables-bs4/js/dataTables.bootstrap4.min.js') }}" ></script>
    <script src="{{ asset('bundles/sbsadminlte/plugins/datatables-responsive/js/dataTables.responsive.min.js') }}" ></script>
    <script src="{{ asset('bundles/sbsadminlte/plugins/datatables-responsive/js/responsive.bootstrap4.min.js') }}" ></script>
    <script src="{{ asset('bundles/sbsadminlte/plugins/datatables-buttons/js/dataTables.buttons.min.js') }}" ></script>
    <script src="{{ asset('bundles/sbsadminlte/plugins/datatables-buttons/js/buttons.bootstrap4.min.js') }}" ></script>
    <script src="{{ asset('bundles/sbsadminlte/plugins/jszip/jszip.min.js') }}" ></script>
    <script src="{{ asset('bundles/sbsadminlte/plugins/pdfmake/pdfmake.min.js') }}" ></script>
    <script src="{{ asset('bundles/sbsadminlte/plugins/pdfmake/vfs_fonts.js') }}" ></script>
    <script src="{{ asset('bundles/sbsadminlte/plugins/datatables-buttons/js/buttons.html5.min.js') }}" ></script>
    <script src="{{ asset('bundles/sbsadminlte/plugins/datatables-buttons/js/buttons.print.min.js') }}" ></script>
    <script src="{{ asset('bundles/sbsadminlte/plugins/datatables-buttons/js/buttons.colVis.min.js') }}" ></script>

    <!-- Select2 -->
    <script src="{{ asset('bundles/sbsadminlte/plugins/select2/js/select2.full.min.js') }}"></script>
    <script src="{{ asset('js/demo.js') }}"></script>

    {% block javascripts_surcharge %}{% endblock %}

   {% endblock %}
